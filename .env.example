# 多智能体框架配置示例
# 复制为 .env 文件并修改相应配置

# OpenRouter API 配置
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_APP_NAME=MultiAgentFramework

# 推理模型配置
REASONING_MODEL=anthropic/claude-3.5-sonnet
# 可选模型:
# - anthropic/claude-3.5-sonnet (推荐，平衡性能)
# - openai/gpt-4-turbo (高质量，成本较高)  
# - anthropic/claude-3-haiku (快速，经济实惠)
# - google/gemini-pro (通用性强)
# - meta-llama/llama-3-70b-instruct (开源方案)

# 智能体特定模型配置（可选覆盖）
COORDINATOR_MODEL=anthropic/claude-3.5-sonnet
PLANNER_MODEL=anthropic/claude-3.5-sonnet  
RESEARCHER_MODEL=anthropic/claude-3-haiku
WRITER_MODEL=openai/gpt-4-turbo
OPTIMIZER_MODEL=anthropic/claude-3.5-sonnet

# 温度参数配置（控制创造性）
COORDINATOR_TEMPERATURE=0.3
PLANNER_TEMPERATURE=0.2
RESEARCHER_TEMPERATURE=0.1
WRITER_TEMPERATURE=0.7
OPTIMIZER_TEMPERATURE=0.4

# 文件处理配置
MAX_FILE_SIZE=52428800  # 50MB
UPLOAD_DIR=./uploads
SESSION_DIR=./sessions

# CORS 配置
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:8005

# 服务器配置
HOST=0.0.0.0
PORT=8001
LOG_LEVEL=info

# 开发模式配置
DEBUG=true
RELOAD=true

# 提示词目录配置
PROMPTS_DIR=./prompts
DEFAULT_PROMPT_EXTENSION=.md

# 缓存配置
CACHE_ENABLED=true
CACHE_TTL=3600  # 1小时

# 重试配置
MAX_RETRIES=3
RETRY_DELAY=1

# 性能配置
MAX_TOKENS=4096
TIMEOUT=30
STREAMING=true